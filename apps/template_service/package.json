{
  "name": "template_service",
  "version": "1.0.0",
  "private": true,
  "description": "Template Service built with NestJS",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "seed:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/database/seed.ts",
    "migrate:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/database/migrate-production.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/typeorm": "^11.0.0",
    "cache-manager": "^7.2.4",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
