# Template Service Configuration

# Service
PORT=4002
NODE_ENV=development

# Database (connects to postgres_template on port 5434)
DB_HOST=localhost
DB_PORT=5434
DB_USER=template_svc
DB_PASSWORD=template_dev_password
DB_NAME=template_service_db

# Redis (for template caching)
REDIS_HOST=localhost
REDIS_PORT=6379

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_QUEUE=template_requests
